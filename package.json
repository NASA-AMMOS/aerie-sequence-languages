{
  "name": "@nasa-jpl/aerie-sequence-languages",
  "version": "0.0.1",
  "description": "Consolidated parsing for languages with first-party support in Aerie sequencing",
  "type": "module",
  "author": "NASA/JPL",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/NASA-AMMOS/aerie-sequence-languages.git"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    }
  },
  "scripts": {
    "test": "vitest run",
    "build": "npm run clean; rollup -c",
    "clean": "rimraf -g ./dist **/*.grammar.js",
    "reformat": "prettier --write ./src"
  },
  "files": [
    "dist/"
  ],
  "keywords": [
    "aerie",
    "satf",
    "sasf",
    "seqN",
    "sequence",
    "language"
  ],
  "devDependencies": {
    "@lezer/generator": "^1.7.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.2",
    "prettier": "^3.5.3",
    "rimraf": "^5.0.5",
    "rollup": "^4.39.0",
    "typescript": "^5.8.2",
    "vitest": "^3.1.1"
  },
  "dependencies": {
    "@lezer/lr": "^1.4.2",
    "@nasa-jpl/aerie-ampcs": "^1.0.6",
    "@nasa-jpl/aerie-time-utils": "git+https://github.com/NASA-AMMOS/aerie-time-utils.git",
    "@nasa-jpl/seq-json-schema": "^1.3.1"
  }
}
